{
  "name": "pre-git",
  "description": "Automatically install pre-commit / pre-git hooks for your npm modules.",
  "version": "0.0.0-semantic-release",
  "author": "Arnout Kazemier <opensource@observe.it>",
  "bin": {
    "commit-wizard": "./bin/commit-wizard"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/pre-git/issues"
  },
  "contributors": [
    "Arnout Kazemier <opensource@observe.it>",
    "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
    "Tauren Mills http://tauren.com"
  ],
  "dependencies": {
    "cz-conventional-changelog": "1.1.4",
    "ggit": "1.2.1",
    "inquirer": "0.11.0",
    "shelljs": "0.3.0",
    "validate-commit-msg": "1.0.0"
  },
  "devDependencies": {
    "git-issues": "1.2.0",
    "grunt": "0.4.5",
    "grunt-bump": "0.0.13",
    "grunt-cli": "0.1.13",
    "grunt-complexity": "0.1.6",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-deps-ok": "0.1.8",
    "grunt-nice-package": "0.9.2",
    "matchdep": "0.3.0",
    "semantic-release": "4.3.5"
  },
  "engines": {
    "node": "> 0.8.*"
  },
  "files": [
    "bin",
    "install.js",
    "hooks",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/bahmutov/pre-git",
  "keywords": [
    "git",
    "pre-commit",
    "precommit",
    "pre-push",
    "test",
    "npm",
    "hooks"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": "grunt",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/pre-git.git"
  },
  "scripts": {
    "build": "grunt",
    "postinstall": "node install",
    "test": "grunt",
    "issues": "git-issues",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
